( $at_check_trace; $COBC -fsyntax-only -free -W -cb_conf=word-length:60 -frelax-syntax prog2.cob
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/syn_misc.at:1784"
$at_failed && at_fn_log_failure
$at_traceon; }



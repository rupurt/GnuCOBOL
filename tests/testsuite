511;run_extensions.at:1940;78 Level (1);extensions;
512;run_extensions.at:1963;78 Level (2);extensions;
513;run_extensions.at:1989;78 Level (3);extensions;
514;run_extensions.at:2013;DEBUG (not active);extensions;
515;run_extensions.at:2036;DEBUG (fdebugging-line);extensions;
516;run_extensions.at:2059;DEBUG (obsolete WITH DEBUGGING MODE);extensions;
517;run_extensions.at:2085;DEBUG free format (not active);extensions;
518;run_extensions.at:2108;DEBUG free format (fdebugging-line);extensions;
519;run_extensions.at:2131;SWITCHES with non-standard names;runmisc extensions;
520;run_extensions.at:2232;Larger REDEFINES lengths;extensions;
521;run_extensions.at:2317;Obsolete 85 keywords;extensions;

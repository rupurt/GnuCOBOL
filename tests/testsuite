physical_cancel notwithme
load_case  insensitive
varseq_format big
sort_chunk 4K
sort_memory 4G # too big by some byte
setenv nothing
sort_chunk
_ATEOF


# conf entries must be clean
{ set +x
$as_echo "$at_srcdir/configuration.at:551: \$COBCRUN -c defunc.cfg --runtime-env"

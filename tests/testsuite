           IF L NOT = 2
              DISPLAY "Length 1 " L
              END-DISPLAY
           END-IF.
           MOVE LENGTH OF Y TO L.
           IF L NOT = 2
              DISPLAY "Length 2 " L
              END-DISPLAY
           END-IF.
           MOVE LENGTH OF Y(1) TO L.
           IF L NOT = 2
              DISPLAY "Length 3 " L
              END-DISPLAY
           END-IF.
           STOP RUN.
_ATEOF



echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/configuration.at:445"
$at_failed && at_fn_log_failure
$at_traceon; }


# check if --runtime-env points to a file called "runtime_empty.cfg"
# use tr to remove newlines and spaces as the path likely is splitted
# into two lines
{ set +x
$as_echo "$at_srcdir/configuration.at:450: \$COBCRUN --runtime-env | tr -d '\\n ' | \\
grep \"runtime_empty.cfg\""
at_fn_check_prepare_notrace 'an embedded newline' "configuration.at:450"
( $at_check_trace; $COBCRUN --runtime-env | tr -d '\n ' | \
grep "runtime_empty.cfg"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_fn_check_status 0 $at_status "$at_srcdir/configuration.at:450"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/configuration.at:452: export COB_RUNTIME_CONFIG=\"\"; \$COBCRUN --runtime-env | tr -d '\\n ' \\
| grep \"runtime.cfg\""
at_fn_check_prepare_notrace 'an embedded newline' "configuration.at:452"
( $at_check_trace; export COB_RUNTIME_CONFIG=""; $COBCRUN --runtime-env | tr -d '\n ' \
| grep "runtime.cfg"
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
